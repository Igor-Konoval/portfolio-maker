              <a href="#contact" class="itemNav">CONTACT</a>
              <div class="log-reg-container dropdown" tabindex="0">
                  <div class="log-reg">
                    <% if (username !== 'non user') { %>
                    <%= username %>
                    <% } else { %>
                      sign in
                    <% } %>
                    </div>
                  <div class="log-list-container">
                    <div class="log-list"> <% if (username !== 'non user') { %>
                      <a href="/cabinet/<%= username %>" class="log-item">my portfolio</a>
                      <a href="/cabinet" class="log-item">edit portfolio</a>
                      <a href="/auth/logout_user" class="log-item">log out</a>
                      <% } else { %>
                      <a href="/form_login" class="log-item">login</a>
                      <a href="/form_registration" class="log-item">registration</a>
                      <% } %>
                    </div>
                  </div>
                </div>

          </div>

          <div class="header-up-text">
              <span class="header_firstline_text" contenteditable="false"> HELLO, I AM </span>
