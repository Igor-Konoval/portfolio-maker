              <div id="container_buttons_header">
                <a class="button form_create" href="/cabinet">Create Portpholio</a>
              </div>
          </div>
          
          <!-- button_redact -->
      </div>
  </div>
  <div class="headerbg">
      <div id="mask-bg"></div>
  </div>

  <script>
      document.addEventListener("DOMContentLoaded", function() {
      let script = document.createElement('script');
      script.src = '/scriptModules/navigator.js';
      document.getElementsByTagName('head')[0].insertBefore(script, document.getElementsByTagName('head')[0].firstChild);
      });

  </script>
  
